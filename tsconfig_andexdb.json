{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "../Debug Sticks/BP/src",
    "rootDir": "../Debug Sticks/BP/src",
    "outDir": "../Debug Sticks/BP/scripts",
    "paths": {
      "@minecraft/math.js": ["../node_modules/@minecraft/math"],
      "@minecraft/vanilla-data.js": ["../node_modules/@minecraft/vanilla-data"]/*,
      "semver": ["C:/Users/ander/github/Debug Sticks/BP/node_modules/semver/index.d.ts"]*/
    },
    "allowJs": true,
    "declaration": true,/*
    "declarationMap": true,*/
    "declarationDir": "../Debug Sticks/BP/scripts",
    "verbatimModuleSyntax": true/*, 
    "resolveJsonModule": true*/,
    "sourceMap": true,
    "noFallthroughCasesInSwitch": true,
    "strictBindCallApply": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "generateCpuProfile": "profile.cpuprofile",
    // "traceResolution": true,
    "extendedDiagnostics": true,
    // "exactOptionalPropertyTypes": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    // "diagnostics": true,
    "skipLibCheck": true,
    "pretty": true,
    "jsx": "react",
    "jsxFactory": "JSX.createElement",
    "jsxFragmentFactory": "JSX.Fragment"
  },
  "exclude": [
    "**/node_modules/@minecraft/server/**",
    "**/node_modules/@minecraft/server-ui/**",
    "**/node_modules/@minecraft/server-gametest/**",
    "**/node_modules/@minecraft/server-admin/**",
    "**/node_modules/@minecraft/server-net/**",
    "**/node_modules/typescript/**"
  ],
  "include": [ "../Debug Sticks/BP/src" ],
}
