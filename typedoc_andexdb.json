{
    "$schema": "https://typedoc.org/schema.json",
    "name": "8Crafter's Debug Sticks API Documentation",
    "navigationLinks": {
        "Add-On Documentation": "https://wiki.8crafter.com/andexdb"
    },
    "favicon": "favicon_andexdb.png",
    "readme": "README_andexdb.md",
    "entryPoints": [
        "../Debug Sticks/BP/src/index.ts",
        "../Debug Sticks/BP/src/typeDocGlobalsFixer.ts" /* , "../Debug Sticks/BP/src/Global.ts" */ /* , "../Debug Sticks/BP/src/modules/**" */
    ],
    "out": "docs/andexdb",
    "jsDocCompatibility": true,
    "basePath": "../Debug Sticks/BP/src",
    "tsconfig": "tsconfig_andexdb.json",
    "useTsLinkResolution": true,
    // "commentStyle": "all",
    "entryPointStrategy": "expand",
    "plugin": [
        "typedoc-plugin-missing-exports",
        "typedoc-plugin-not-exported",
        "typedoc-plugin-8crafter-type-converter",
        "typedoc-plugin-merge-modules",
        "typedoc-plugin-coverage"/* ,
        "@8hobbies/typedoc-plugin-404" */
    ],
    "exclude": [
        // "**/node_modules/@minecraft/server/**",
        // "**/node_modules/@minecraft/server-ui/**",
        // "**/node_modules/@minecraft/server-gametest/**",
        // "**/node_modules/@minecraft/server-admin/**",
        // "**/node_modules/@minecraft/server-net/**",
        "**/node_modules/typescript/**"
    ],
    "cleanOutputDir": true,
    "externalSymbolLinkMappings": {
        "@minecraft/server": {
            "*": "https://www.npmjs.com/package/@minecraft/server"
        },
        "@minecraft/server-ui": {
            "*": "https://www.npmjs.com/package/@minecraft/server-ui"
        },
        "@minecraft/server-gametest": {
            "*": "https://www.npmjs.com/package/@minecraft/server-gametest"
        },
        "@minecraft/server-admin": {
            "*": "https://www.npmjs.com/package/@minecraft/server-admin"
        },
        "@minecraft/server-net": {
            "*": "https://www.npmjs.com/package/@minecraft/server-net"
        }
    },
    "excludeNotDocumented": false,
    "excludeReferences": false,
    "excludeExternals": false,
    "excludeProtected": false,
    "excludePrivate": false,
    "excludeInternal": false,
    "excludeTags": ["@override", "@virtual", "@privateRemarks", "@satisfies", "@overload", "@inline"],
    "visibilityFilters": {
        "protected": false,
        "private": false,
        "inherited": true,
        "external": false
    },
    "blockTags": [
        "@defaultValue",
        "@deprecated",
        "@example",
        "@param",
        "@privateRemarks",
        "@remarks",
        "@returns",
        "@see",
        "@throws",
        "@typeParam",
        "@author",
        "@callback",
        "@category",
        "@categoryDescription",
        "@default",
        "@document",
        "@extends",
        "@augments",
        "@yields",
        "@group",
        "@groupDescription",
        "@import",
        "@inheritDoc",
        "@jsx",
        "@license",
        "@module",
        "@mergeModuleWith",
        "@prop",
        "@property",
        "@return",
        "@satisfies",
        "@since",
        "@template",
        "@type",
        "@typedef",
        "@summary",
        "@version",
        "@warning",
        "@danger",
        "@todo",
        "@note",
        "@async",
        "@mutable"
    ],
    "externalPattern": [
        // "**/node_modules/[!c]*/**",
        // "**/node_modules/c[!o]*/**",
        // "**/node_modules/co[!l]*/**",
        // "**/node_modules/col[!o]*/**",
        // "**/node_modules/colo[!r]*/**",
        // "**/node_modules/color[!\\-]*/**",
        // "**/node_modules/color-[!c]*/**",
        // "**/node_modules/color-c[!o]*/**",
        // "**/node_modules/color-co[!r]*/**",
        // "**/node_modules/color-cor[!e]*/**",
        "**/node_modules/@minecraft/server/**",
        "**/node_modules/@minecraft/server-ui/**",
        "**/node_modules/@minecraft/server-gametest/**",
        "**/node_modules/@minecraft/server-admin/**",
        "**/node_modules/@minecraft/server-net/**",
        "**/node_modules/.bin/**",
        "**/node_modules/@esbuild/**",
        "**/node_modules/asynckit/**",
        "**/node_modules/axios/**",
        "**/node_modules/combined-stream/**",
        "**/node_modules/delayed-stream/**",
        "**/node_modules/esbuild/**",
        "**/node_modules/follow-redirects/**",
        "**/node_modules/form-data/**",
        "**/node_modules/mime-db/**",
        "**/node_modules/mime-types/**",
        "**/node_modules/proxy-from-env/**",
        "**/node_modules/typescript/**"
    ],
    "mergeModulesMergeMode": "module",
    "kindSortOrder": [
        "Project",
        "Module",
        "Namespace",
        "Enum",
        "EnumMember",
        "Class",
        "Interface",
        "TypeAlias",
        "Constructor",
        "Property",
        "Variable",
        "Function",
        "Accessor",
        "Method",
        "Parameter",
        "TypeParameter",
        "TypeLiteral",
        "CallSignature",
        "ConstructorSignature",
        "IndexSignature",
        "GetSignature",
        "SetSignature",
        "Reference"
    ],
    "groupOrder": [
        "Projects",
        "Modules",
        "Namespaces",
        "Enumerations",
        "Enumeration Members",
        "Classes",
        "Interfaces",
        "Type Aliases",
        "Constructors",
        "Getters",
        "Setters",
        "Properties",
        "Variables",
        "Functions",
        "Accessors",
        "Methods",
        "Parameters",
        "Type Parameters",
        "Type Literals",
        "Call Signatures",
        "Constructor Signatures",
        "Index Signatures",
        "Get Signatures",
        "Set Signatures",
        "*",
        "References"
    ],
    "modifierTags": [
        "@alpha",
        "@beta",
        "@eventProperty",
        "@experimental",
        "@internal",
        "@override",
        "@packageDocumentation",
        "@public",
        "@readonly",
        "@sealed",
        "@virtual",
        "@abstract",
        "@class",
        "@enum",
        "@event",
        "@expand",
        "@hidden",
        "@hideCategories",
        "@hideconstructor",
        "@hideGroups",
        "@ignore",
        "@inline",
        "@interface",
        "@namespace",
        "@overload",
        "@private",
        "@protected",
        "@showCategories",
        "@showGroups",
        "@useDeclaredType"
    ],
    "hostedBaseUrl": "https://api.8crafter.com/andexdb",
    "useHostedBaseUrlForAbsoluteLinks": false,
    "currentAPIDocDirectory": "andexdb"
}
